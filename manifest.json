{
  "name": "Local Storage Catcher",
  "description": "A facilitator of access to local storage keys with JSON beautify",
	"version": "2.0.1",
	"manifest_version": 3,
	"background": {
    "service_worker": "scripts/background.js"
  },
	"permissions": ["storage", "tabs", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "128.png"
    }
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "icons": {
    "128": "128.png"
  },
  "host_permissions": [
    "*://*/*"
  ]
}